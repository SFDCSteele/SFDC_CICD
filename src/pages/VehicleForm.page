<apex:page standardController="Vehicle__c"  extensions="VehicleCreation" sidebar="false">
    
           <div id="newVehicle" >
                <apex:form >
                <apex:pageMessages id="errors" />
                <apex:pageBlock title="Create Vehicle" mode="edit">
                    <apex:pageBlockSection columns="1">
                        <apex:inputField value="{! vehicle.Model_Year__c }"/>
                        <apex:inputField value="{! vehicle.Model_Make__c }"/>
                        <apex:inputField value="{! vehicle.Model__c }"/>
                        <apex:inputField value="{! vehicle.VIN__c }"/>
                    </apex:pageBlockSection>
            
                    <apex:pageBlockButtons location="top">
                        <apex:commandButton action="{! saveVehicle }" value="Save" />        
                   </apex:pageBlockButtons>
            
                    <apex:pageBlock id="hiddenBlock1" rendered="false"></apex:pageBlock>
            
                    </apex:pageBlock>
                </apex:form>
          </div>

</apex:page>